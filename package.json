{
  "name": "webpack-boilerplate",
  "version": "2.0.0",
  "description": "Minimalistic ES6 boilerplate",
  "main": "index.js",
  "scripts": {
    "production": "npm run build",
    "start": "webpack-dev-server --hot",
    "clean": "rimraf dist",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --config webpack.production.config.js --progress --profile --colors",
    "lint": "eslint app",
    "test": "mocha --compilers js:babel-core/register --recursive ./tests/**/*.js",
    "precommit": "npm run lint && npm test",
    "prepush": "npm run lint && npm test"
  },
  "author": "sSolov",
  "license": "MIT",
  "dependencies": {
    "cross-env": "^3.1.4",
    "history": "^4.5.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.6",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-2": "^6.13.0",
    "compression-webpack-plugin": "^0.3.2",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "extract-loader": "^0.1.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "file-loader": "^0.10.0",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.26.0",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.2",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  }
}
